{
    "name": "rgalura/api-igniter",
    "version": "1.0.0",
    "description": "A Laravel package to enable your API's more powerful",
    "type": "library",
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "laravel/pint": "^1.21",
        "pestphp/pest": "^3.7",
        "orchestra/testbench": "^10.1",
        "pestphp/pest-plugin-laravel": "^3.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RGalura\\ApiIgniter\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Rodrigo Galura",
            "email": "rodrigogalura3rd@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
